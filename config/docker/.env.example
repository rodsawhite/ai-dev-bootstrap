# Docker Compose Environment Variables
# Copy this file to .env and fill in the values before using docker-compose

# Project name (automatically set by bootstrap)
# COMPOSE_PROJECT_NAME=yourname-ai-dev

# Project directory to mount
# PROJECT_DIR=./project

# PostgreSQL credentials (required when using 'database' profile)
POSTGRES_USER=developer
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=devdb
POSTGRES_PORT=5432

# MinIO credentials (required when using 'storage' profile)
MINIO_USER=minioadmin
MINIO_PASSWORD=your-secure-password-here
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001

# Redis port (optional)
REDIS_PORT=6379

# API keys for AI agents (passed to dev container)
# ANTHROPIC_API_KEY=sk-ant-...
# OPENAI_API_KEY=sk-...
